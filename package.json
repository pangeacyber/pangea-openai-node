{
  "name": "@pangeacyber/openai",
  "version": "0.1.0",
  "description": "A wrapper around the OpenAI TypeScript library that wraps the Responses API with Pangea AI Guard",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE.txt",
    "README.md"
  ],
  "engines": {
    "node": "22 || >=24"
  },
  "scripts": {
    "attw": "attw --pack",
    "publint": "publint --strict",
    "build": "tsdown",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "packageManager": "pnpm@10.16.0",
  "dependencies": {
    "openai": "^5.20.1",
    "pangea-node-sdk": "^5.5.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.2.4",
    "@kenan/biome-config": "1.0.4",
    "@tsconfig/node22": "22.0.2",
    "@types/node": "22.18.6",
    "lefthook": "1.13.0",
    "publint": "0.3.12",
    "tsdown": "0.15.1",
    "typescript": "5.9.2"
  }
}
